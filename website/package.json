{
  "name": "awesome-github-readme-templates",
  "private": true,
  "description": "A collection of awesome readme templates",
  "version": "1.0.0",
  "author": "Elangovan",
  "bugs": {
    "url": "https://github.com/elangosundar/awesome-README-templates/issues"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bootstrap": "^4.5.0",
    "gatsby": "^2.0.76",
    "gatsby-plugin-feed": "^2.0.8",
    "gatsby-plugin-google-analytics": "^2.0.5",
    "gatsby-plugin-manifest": "^2.0.5",
    "gatsby-plugin-offline": "^3.0.30",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.16",
    "gatsby-plugin-sitemap": "^2.0.4",
    "gatsby-plugin-typography": "^2.2.0",
    "gatsby-remark-autolink-headers": "^2.0.12",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^3.1.39",
    "gatsby-remark-prismjs": "^3.0.0",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.12",
    "gatsby-transformer-remark": "^2.1.17",
    "gatsby-transformer-sharp": "^2.1.9",
    "lodash": "^4.17.11",
    "prismjs": "^1.15.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-image-fallback": "^8.0.0",
    "react-typography": "^0.16.13",
    "typeface-merriweather": "^0.0.72",
    "typeface-montserrat": "^0.0.75",
    "typography": "^0.16.17",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "env-cmd": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.1.1",
    "prettier": "^1.14.2"
  },
  "homepage": "https://github.com/elangosundar/awesome-README-templates",
  "license": "MIT",
  "scripts": {
    "dev": "env-cmd -f .env.development gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "build": "rm -rf public && rm -rf .cache && gatsby build",
    "deploy": "yarn build && cd public && now alias $(now) awesome-github-profile",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "git-deploy": "gatsby build --prefix-paths && gh-pages -d public"
  }
}
